{
  "version": "1.0",
  "services": {
    "web": {
      "image": "feed-service",
      "port": 3030,
      "volumes": [
        {
          "name": "cursor",
          "destinationPath": "/cursor"
        }
      ]
    },
    "worker": {
      "image": "appview",
      "volumes": [
        {
          "name": "cursor",
          "destinationPath": "/cursor"
        }
      ]
    }
  },
  "images": {
    "feed-service": {
      "dockerfile": "feeds/Dockerfile"
    },
    "appview": {
      "dockerfile": "appview/Dockerfile"
    }
  }
}
